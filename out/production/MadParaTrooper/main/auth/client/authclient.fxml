<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.auth.client.AuthClientController">
<center>
    <TableView fx:id="iplist">
        <columns>
            <TableColumn fx:id="column_id" maxWidth="500.0" minWidth="50.0" prefWidth="120.0" text="ID" />
            <TableColumn fx:id="column_ip" prefWidth="184.0" text="IP" />
            <TableColumn fx:id="column_state" maxWidth="100.0" minWidth="80.0" prefWidth="100.0" text="state" />
        </columns>
        <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" top="20.0" />
        </BorderPane.margin>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</center>
<bottom>
    <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </BorderPane.margin>
         <children>
            <Button fx:id="refresh" mnemonicParsing="false" onAction="#refresh" text="refresh" />
         </children>
    </HBox>
</bottom>
<right>
    <TextArea fx:id="console" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true" BorderPane.alignment="CENTER">
        <BorderPane.margin>
            <Insets bottom="20.0" left="10.0" right="20.0" top="20.0" />
        </BorderPane.margin>
    </TextArea>
</right>
</BorderPane>
